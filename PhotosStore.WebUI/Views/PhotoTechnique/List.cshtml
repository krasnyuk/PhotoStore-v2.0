@using PhotosStore.WebUI.HtmlHelpers
@using PhotosStore.WebUI.Models

@model PhotoTechniqueListViewModel

@{
    ViewBag.Title = "Товары - список";
}

@foreach (var technique in @Model.PhotoTechniques)
{
    @Html.Partial("PhotoTechniqueSummary", technique)
}

<div class="btn-group pull-right">
    @Html.PageLinks(Model.PagingInfo, x => Url.Action("List", new { page = x, category = Model.CurrentCategory }))
</div>

<script>
    $('#List').addClass('active');
    $('#Mesh').removeClass('active');
</script>